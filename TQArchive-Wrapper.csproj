		<RootNamespace>TQArchive_Wrapper</RootNamespace>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>ByteSquire</Authors>
		<Company>ByteSquire.tq</Company>
		<Product>TQArchive-Wrapper</Product>
		<Version>2.1.0</Version>
		<PackageId>$(Company).$(Product)</PackageId>
