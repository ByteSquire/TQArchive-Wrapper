	<ItemGroup>
		<PackageReference Include="System.Text.Encoding.CodePages" Version="6.0.0" />
		<PackageReference Include="ByteSquire.tq.TQDB-Parser" Version="1.3.5" />
	</ItemGroup>

</Project>
